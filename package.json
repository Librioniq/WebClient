{
    "name": "librioniq",
    "version": "0.1.0",
    "description": "",
    "repository": {
        "type": "git",
        "url": ""
    },
    "scripts": {
        "star-old": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
        "start": "node server/production",
        "start-ts": "ts-node src/main/server",
        "start-prod": "better-npm-run start-prod",
        "start-prod-api": "better-npm-run start-prod-api",
        "build": "npm run build:prod",
        "build:dev": "better-npm-run build-dev",
        "build:prod": "better-npm-run build-prod",
        "lint": "tsconfig-lint",
        "start-dev": "better-npm-run start-dev",
        "start-dev-api": "better-npm-run start-dev-api",
        "watch-client": "better-npm-run watch-client",
        "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
        "test": "mocha --recursive ./src/test/**/*.ts --require ts-node/register ./src/test/**/*.ts",
        "test-watch": "mocha --recursive ./src/test/**/*.ts --require ts-node/register ./src/test/**/*.ts --watch"
    },
    "betterScripts": {
        "start-prod": {
            "command": "node ./bin/server.js",
            "env": {
                "NODE_PATH": "./src",
                "NODE_ENV": "production",
                "PORT": 8080,
                "APIPORT": 3030
            }
        },
        "start-prod-api": {
            "command": "node ./bin/api.js",
            "env": {
                "NODE_PATH": "./api",
                "NODE_ENV": "production",
                "APIPORT": 3030
            }
        },
        "start-dev": {
            "command": "node ./bin/server.js",
            "env": {
                "NODE_PATH": "./src",
                "NODE_ENV": "development",
                "PORT": 3000,
                "APIPORT": 3030
            }
        },
        "start-dev-api": {
            "command": "node ./bin/api.js",
            "env": {
                "NODE_PATH": "./api",
                "NODE_ENV": "development",
                "APIPORT": 3030
            }
        },
        "watch-client": {
            "command": "node webpack/webpack-dev-server.js",
            "env": {
                "UV_THREADPOOL_SIZE": 100,
                "NODE_PATH": "./src",
                "PORT": 3000,
                "APIPORT": 3030
            }
        },
        "build-prod": {
            "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "build-dev": {
            "command": "webpack --verbose --colors --display-error-details --config webpack/dev.config.js",
            "env": {
                "NODE_ENV": "production"
            }
        }
    },
    "dependencies": {
        "classnames": "^2.2.3",
        "es6-promise": "^3.1.2",
        "history": "^2.0.1",
        "isomorphic-fetch": "^2.2.1",
        "lodash": "^4.6.1",
        "react": "^0.14.7",
        "react-dom": "^0.14.7",
        "react-redux": "^4.4.1",
        "react-router": "^2.0.1",
        "react-router-redux": "^4.0.0",
        "redux": "^3.3.1",
        "redux-actions": "^0.9.1",
        "redux-thunk": "^2.0.1",
        "typescript": "^1.8.7"
    },
    "devDependencies": {
        "ts-node": "latest",
        "body-parser": "latest",
        "express": "latest",
        "express-session": "latest",
        "serve-static": "latest",
        "webpack-isomorphic-tools": "^2.2.31",
        "extract-text-webpack-plugin": "^1.0.1",
        "url-loader": "^0.5.7",
        "autoprefixer-loader": "^3.2.0",
        "babel": "6.3.26",
        "babel-core": "6.4.0",
        "babel-loader": "6.2.1",
        "babel-polyfill": "6.3.14",
        "babel-preset-es2015": "6.3.13",
        "babel-runtime": "6.3.19",
        "better-npm-run": "0.0.8",
        "bootstrap-sass": "^3.3.6",
        "bootstrap-sass-loader": "^1.0.10",
        "clean-webpack-plugin": "^0.1.6",
        "css-loader": "^0.23.1",
        "eslint-plugin-react": "3.8.0",
        "file-loader": "^0.8.5",
        "font-awesome": "^4.5.0",
        "font-awesome-webpack": "^0.0.4",
        "jsdom": "7.0.2",
        "json-loader": "^0.5.4",
        "less": "^2.5.3",
        "node-sass": "^3.4.2",
        "less-loader": "^2.2.2",
        "chai": "latest",
        "mocha": "latest",
        "react-hot-loader": "1.3.0",
        "redux-devtools": "latest",
        "redux-devtools-log-monitor": "latest",
        "redux-devtools-dock-monitor": "latest",
        "sass-loader": "3.1.2",
        "superagent": "latest",
        "supertest": "latest",
        "fetch-mock": "latest",
        "sinon": "latest",
        "stats-webpack-plugin": "0.1.2",
        "strip-loader": "^0.1.2",
        "style-loader": "0.13.0",
        "ts-loader": "0.6.1",
        "tslint": "latest",
        "tslint-loader": "latest",
        "tsconfig-lint": "latest",
        "typescript": "latest",
        "typescript-require": "0.2.9-1",
        "webpack": "1.12.10",
        "webpack-dev-middleware": "^1.4.0",
        "webpack-dev-server": "1.14.0",
        "webpack-hot-middleware": "^2.5.0"
    },
    "engines": {
        "node": "5.8.0"
    }
}
