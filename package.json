{
    "name": "librioniq",
    "version": "0.1.0",
    "description": "",
    "repository": {
        "type": "git",
        "url": ""
    },
    "scripts": {
        "star-old": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
        "start": "node server/production",
        "start-prod": "better-npm-run start-prod",
        "start-prod-api": "better-npm-run start-prod-api",
        "build": "better-npm-run build",
        "postinstall": "npm run build",
        "lint": "tsconfig-lint",
        "start-dev": "better-npm-run start-dev",
        "start-dev-api": "better-npm-run start-dev-api",
        "watch-client": "better-npm-run watch-client",
        "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
        "test": "karma start",
        "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
        "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
    },
    "betterScripts": {
        "start-prod": {
            "command": "node ./bin/server.js",
            "env": {
                "NODE_PATH": "./src",
                "NODE_ENV": "production",
                "PORT": 8080,
                "APIPORT": 3030
            }
        },
        "start-prod-api": {
            "command": "node ./bin/api.js",
            "env": {
                "NODE_PATH": "./api",
                "NODE_ENV": "production",
                "APIPORT": 3030
            }
        },
        "start-dev": {
            "command": "node ./bin/server.js",
            "env": {
                "NODE_PATH": "./src",
                "NODE_ENV": "development",
                "PORT": 3000,
                "APIPORT": 3030
            }
        },
        "start-dev-api": {
            "command": "node ./bin/api.js",
            "env": {
                "NODE_PATH": "./api",
                "NODE_ENV": "development",
                "APIPORT": 3030
            }
        },
        "watch-client": {
            "command": "node webpack/webpack-dev-server.js",
            "env": {
                "UV_THREADPOOL_SIZE": 100,
                "NODE_PATH": "./src",
                "PORT": 3000,
                "APIPORT": 3030
            }
        },
        "build": {
            "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
            "env": {
                "NODE_ENV": "production"
            }
        }
    },
    "dependencies": {
        "classnames": "2.2.3",
        "express": "4.13.3",
        "extract-text-webpack-plugin": "0.9.1",
        "lodash": "3.10.1",
        "react": "0.14.7",
        "react-dom": "0.14.7",
        "react-redux": "4.4.0",
        "react-router": "2.0.0",
        "history": "2.0.0",
        "react-router-redux": "4.0.0",
        "redux": "3.3.1",
        "redux-actions": "0.9.1",
        "serve-static": "1.10.0",
        "typescript": "1.8.7",
        "webpack-isomorphic-tools": "^2.2.18",
        "yargs": "3.29.0"
    },
    "devDependencies": {
        "url-loader": "^0.5.7",
        "autoprefixer-loader": "^3.2.0",
        "babel": "6.3.26",
        "babel-core": "6.4.0",
        "babel-loader": "6.2.1",
        "babel-polyfill": "6.3.14",
        "babel-preset-es2015": "6.3.13",
        "babel-runtime": "6.3.19",
        "better-npm-run": "0.0.8",
        "bootstrap-sass": "^3.3.6",
        "bootstrap-sass-loader": "^1.0.10",
        "chai": "3.4.1",
        "clean-webpack-plugin": "^0.1.6",
        "css-loader": "^0.23.1",
        "eslint-plugin-react": "3.8.0",
        "file-loader": "^0.8.5",
        "font-awesome": "^4.5.0",
        "font-awesome-webpack": "^0.0.4",
        "jsdom": "7.0.2",
        "json-loader": "^0.5.4",
        "less": "^2.5.3",
        "less-loader": "^2.2.2",
        "mocha": "2.3.3",
        "node-sass": "^3.4.2",
        "react-hot-loader": "1.3.0",
        "redux-devtools": "2.1.5",
        "sass-loader": "3.1.2",
        "socket.io": "1.3.7",
        "stats-webpack-plugin": "0.1.2",
        "strip-loader": "^0.1.2",
        "style-loader": "0.13.0",
        "ts-loader": "0.6.1",
        "tslint": "latest",
        "tslint-loader": "2.1.3",
        "tsconfig-lint": "latest",
        "typescript": "1.8.7",
        "typescript-require": "0.2.9-1",
        "webpack": "1.12.10",
        "webpack-dev-middleware": "^1.4.0",
        "webpack-dev-server": "1.14.0",
        "webpack-hot-middleware": "^2.5.0"
    },
    "engines": {
        "node": "5.6.0"
    }
}